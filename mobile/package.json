{
  "name": "ubuntu-health-mobile",
  "version": "0.1.0",
  "description": "Ubuntu Health Mobile App - Offline-capable healthcare access with Ubuntu philosophy",
  "private": true,
  "main": "src/App.tsx",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:ios": "cd ios && xcodebuild -workspace UbuntuHealth.xcworkspace -scheme UbuntuHealth -configuration Release -destination generic/platform=iOS -archivePath UbuntuHealth.xcarchive archive",
    "ubuntu-setup": "node scripts/ubuntu-mobile-setup.js"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.72.6",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "react-native-safe-area-context": "^4.7.4",
    "react-native-screens": "^3.27.0",
    "react-native-gesture-handler": "^2.13.4",
    "@solana/web3.js": "^1.78.4",
    "react-native-keychain": "^8.1.3",
    "react-native-biometrics": "^3.0.1",
    "react-native-camera": "^4.2.1",
    "@react-native-async-storage/async-storage": "^1.19.4",
    "react-native-voice": "^3.2.4",
    "react-native-document-picker": "^9.1.1",
    "react-native-fs": "^2.20.0",
    "react-native-svg": "^13.14.0",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-vector-icons": "^10.0.2",
    "react-native-toast-message": "^2.1.7",
    "react-native-modal": "^13.0.1",
    "react-native-qrcode-svg": "^6.2.0",
    "react-native-qrcode-scanner": "^1.5.5",
    "react-query": "^3.39.3",
    "axios": "^1.6.0",
    "date-fns": "^2.30.0",
    "react-i18next": "^13.5.0",
    "i18next": "^23.6.0",
    "zustand": "^4.4.6"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@babel/runtime": "^7.23.2",
    "@react-native/eslint-config": "^0.73.1",
    "@react-native/metro-config": "^0.73.2",
    "@tsconfig/react-native": "^3.0.2",
    "@types/react": "^18.2.25",
    "@types/react-test-renderer": "^18.0.4",
    "babel-jest": "^29.7.0",
    "eslint": "^8.52.0",
    "jest": "^29.7.0",
    "metro-react-native-babel-preset": "0.77.0",
    "prettier": "^3.0.3",
    "react-test-renderer": "18.2.0",
    "typescript": "^5.2.2"
  },
  "keywords": [
    "ubuntu",
    "health",
    "mobile",
    "react-native",
    "offline",
    "healthcare",
    "community"
  ],
  "author": "Ubuntu Health Community",
  "license": "MIT",
  "engines": {
    "node": ">=18"
  }
}
